{
  "name": "scrapper",
  "version": "1.0.0",
  "description": "A test project",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "watch": "nodemon --config ./config/nodemon.config.json",
    "prod": "cross-env NODE_ENV=production npm-run-all babel:run",
    "dev": "cross-env NODE_ENV=development npm-run-all babel:run",
    "server:prod": "node ./dist/server",
    "babel:run": "babel-node ./src/server",
    "babel:build": "babel src/server.js --out-dir dist",
    "build": "npm-run-all clean babel:build",
    "clean": "rimraf dist",
    "debug": "nodemon --config config/nodemon.config --inspect ./src/server",
    "client": "cd src/client && npm start",
    "pm2": "pm2 start config/pm2.process.config.js --env production",
    "client-build": "cd src/client && npm run build"
  },
  "author": "Saidul Amin",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "aws-sdk": "^2.814.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.5",
    "express-validator": "^6.3.1",
    "jimp": "^0.9.3",
    "mkdirp": "^0.5.1",
    "mongoose": "^5.8.6",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-geocoder": "^3.25.0",
    "puppeteer": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "cross-env": "^6.0.3",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0"
  }
}
